pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is the second day of building on Jenkins'
            }
        }
        stage('Test') {
            steps {
                echo 'This is the second day of testing on Jenkins'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is the second day of deploying on Jenkins'
            }
        }
    }
}

